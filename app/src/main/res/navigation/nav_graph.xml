<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    >

    <fragment
        android:id="@+id/inicioFragment"
        android:name="com.example.yourfish.MainActivity"
        android:label="Fragment_inicio"
        tools:layout="@layout/activity_main">
        <action
            android:id="@+id/action_inicioFragment_to_primerFragment"
            app:destination="@id/primerFragment" />
        <action
            android:id="@+id/action_inicioFragment_to_segundoFragment"
            app:destination="@id/segundoFragment" />
    </fragment>

    <fragment
        android:id="@+id/primerFragment"
        android:name="com.example.yourfish.fragment_carro"
        android:label="Fragment_inicio"
        tools:layout="@layout/fragment_carro"/>

    <fragment
        android:id="@+id/segundoFragment"
        android:name="com.example.yourfish.fragment_home"
        android:label="Fragment_inicio"
        tools:layout="@layout/fragment_home"/>

</navigation>